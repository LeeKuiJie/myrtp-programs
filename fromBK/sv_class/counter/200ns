#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55a19fe2b650 .scope module, "tb_counter_4bit" "tb_counter_4bit" 2 1;
 .timescale 0 0;
v0x55a19fe3d3b0_0 .var "clk", 0 0;
v0x55a19fe3d480_0 .net "count_out", 3 0, v0x55a19fe3d180_0;  1 drivers
v0x55a19fe3d550_0 .var "rstn", 0 0;
S_0x55a19fe2b7d0 .scope module, "dut" "counter_4bit" 2 9, 3 1 0, S_0x55a19fe2b650;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "rstn"
    .port_info 2 /OUTPUT 4 "count_out"
v0x55a19fdf50a0_0 .net "clk", 0 0, v0x55a19fe3d3b0_0;  1 drivers
v0x55a19fe3d180_0 .var "count_out", 3 0;
v0x55a19fe3d260_0 .net "rstn", 0 0, v0x55a19fe3d550_0;  1 drivers
E_0x55a19fe08840/0 .event negedge, v0x55a19fe3d260_0;
E_0x55a19fe08840/1 .event posedge, v0x55a19fdf50a0_0;
E_0x55a19fe08840 .event/or E_0x55a19fe08840/0, E_0x55a19fe08840/1;
    .scope S_0x55a19fe2b7d0;
T_0 ;
    %wait E_0x55a19fe08840;
    %load/vec4 v0x55a19fe3d260_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55a19fe3d180_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55a19fe3d180_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55a19fe3d180_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55a19fe2b650;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55a19fe3d3b0_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x55a19fe3d3b0_0;
    %inv;
    %store/vec4 v0x55a19fe3d3b0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x55a19fe2b650;
T_2 ;
    %vpi_call 2 25 "$dumpfile", "tb_counter.vcd" {0 0 0};
    %vpi_call 2 26 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55a19fe2b650 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55a19fe3d550_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55a19fe3d550_0, 0, 1;
    %delay 300, 0;
    %vpi_call 2 39 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55a19fe2b650;
T_3 ;
    %vpi_call 2 44 "$monitor", "Time=%0tns, clk=%0b, rstn=%0b, count_out=%0d", $time, v0x55a19fe3d3b0_0, v0x55a19fe3d550_0, v0x55a19fe3d480_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "4bit_counter_tb.v";
    "4bit_counter.v";
