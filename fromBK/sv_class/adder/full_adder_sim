S_0x56416bc0f1d0 .scope module, "full_adder_tb" "full_adder_tb" 2 1;
 .timescale 0 0;
v0x56416bc23360_0 .var "a_tb", 0 0;
v0x56416bc23420_0 .var "b_tb", 0 0;
v0x56416bc234f0_0 .var "cin_tb", 0 0;
v0x56416bc235f0_0 .net "cout_tb", 0 0, L_0x56416bc23d40;  1 drivers
v0x56416bc236c0_0 .var/i "i", 31 0;
v0x56416bc23760_0 .net "sum_tb", 0 0, L_0x56416bc23940;  1 drivers
S_0x56416bc0f350 .scope module, "dut" "full_adder" 2 10, 3 1 0, S_0x56416bc0f1d0;
