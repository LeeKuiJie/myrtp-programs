v0x56416bc22fc0_0 .net "b", 0 0, v0x56416bc23420_0;  1 drivers
v0x56416bc23080_0 .net "cin", 0 0, v0x56416bc234f0_0;  1 drivers
v0x56416bc23140_0 .net "cout", 0 0, L_0x56416bc23d40;  alias, 1 drivers
v0x56416bc23200_0 .net "sum", 0 0, L_0x56416bc23940;  alias, 1 drivers
    .scope S_0x56416bc0f1d0;
T_0 ;
    %vpi_call 2 23 "$dumpfile", "full_adder.vcd" {0 0 0};
    %vpi_call 2 24 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x56416bc0f1d0 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x56416bc236c0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x56416bc236c0_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x56416bc236c0_0;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %pushi/vec4 1, 0, 32;
    %and;
    %pad/s 1;
    %store/vec4 v0x56416bc23360_0, 0, 1;
    %load/vec4 v0x56416bc236c0_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %pushi/vec4 1, 0, 32;
    %and;
    %pad/s 1;
    %store/vec4 v0x56416bc23420_0, 0, 1;
    %load/vec4 v0x56416bc236c0_0;
    %pushi/vec4 1, 0, 32;
    %and;
    %pad/s 1;
    %store/vec4 v0x56416bc234f0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 36 "$display", "A=%0b, B=%0b, Cin=%0b | Sum=%0b, Cout=%0b", v0x56416bc23360_0, v0x56416bc23420_0, v0x56416bc234f0_0, v0x56416bc23760_0, v0x56416bc235f0_0 {0 0 0};
    %load/vec4 v0x56416bc236c0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x56416bc236c0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %vpi_call 2 46 "$finish" {0 0 0};
    %end;
    .thread T_0;
