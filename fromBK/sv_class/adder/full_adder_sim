    .port_info 0 /INPUT 1 "a"
    .port_info 1 /INPUT 1 "b"
    .port_info 2 /INPUT 1 "cin"
    .port_info 3 /OUTPUT 1 "sum"
    .port_info 4 /OUTPUT 1 "cout"
L_0x56416bc23800 .functor XOR 1, v0x56416bc23360_0, v0x56416bc23420_0, C4<0>, C4<0>;
L_0x56416bc23940 .functor XOR 1, L_0x56416bc23800, v0x56416bc234f0_0, C4<0>, C4<0>;
